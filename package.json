{
    "name": "cep-ui",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@types/history": "^4.7.8",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.14.0",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@types/uuid": "^8.3.0",
        "query-string": "^6.13.6",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.3",
        "react-test-renderer": "^16.14.0",
        "typescript": "^3.9.7",
        "uuid": "^8.3.1"
    },
    "scripts": {
        "start": "chmod +x ./env.sh && ./env.sh && mv -f env-config.js ./public/ && react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=true --env=jest-environment-jsdom-sixteen",
        "test:coverage": "react-scripts test --coverage --watchAll=true --env=jest-environment-jsdom-sixteen",
        "eject": "react-scripts eject",
        "test-ci": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen --collect-coverage -u",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/react-hooks": "^3.4.2",
        "@testing-library/user-event": "^12.1.8",
        "@types/react-router-dom": "^5.1.6",
        "jest-environment-jsdom-sixteen": "^1.0.3",
        "nock": "^13.0.4",
        "prettier": "^2.1.2"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/*.{ts,tsx}",
            "!src/test-utils/*.{ts,tsx}"
        ]
    }
}
